<config>
    <moduleName>tkdraw</moduleName>
    <parentModules>
        <module>tkernel</module>
        <module>tkmath</module>
        <module>tkservice</module>
        <module>tkg2d</module>
        <module>tkg3d</module>
        <module>tkgeombase</module>
        <module>tkbrep</module>
        <module>tkgeomalgo</module>
        <module>tkshhealing</module>
        <module>tkhlr</module>
        <module>tkmesh</module>
    </parentModules>
    <parts>1</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <functions>
        <!-- <name>^IBPMatrix.*</name> -->
    </functions>
    <exclude>
        <!-- is implicitly deleted -->
        <signature>.*Draw_ProgressIndicator\(const Draw_ProgressIndicator&amp;\).*</signature>
        <!-- OS X internal stuff -->
        <signature>.*qt_mac_set_cursor.*</signature>
    </exclude>
    <classList>
