<config>
    <moduleName>tkxdedraw</moduleName>
    <parentModules>
        <module>tkxsdraw</module>
    </parentModules>
    <parts>1</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <functions>
        <!-- <name>^IBPMatrix.*</name> -->
    </functions>
    <exclude>
        <!-- <signature>.*::Iterator.*</signature> -->
        <!-- OS X internal stuff -->
        <signature>.*qt_mac_set_cursor.*</signature>
    </exclude>
    <classList>
        <class>XDEDRAW</class>
        <class>XDEDRAW_Colors</class>
        <class>XDEDRAW_Common</class>
        <class>XDEDRAW_GDTs</class>
        <class>XDEDRAW_Layers</class>
        <class>XDEDRAW_Notes</class>
        <class>XDEDRAW_Props</class>
        <class>XDEDRAW_Shapes</class>
        <class>XDEDRAW_Views</class>
    </classList>
</config>
