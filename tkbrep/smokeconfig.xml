<config>
    <moduleName>tkbrep</moduleName>
    <parentModules>
        <module>tkernel</module>
        <module>tkmath</module>
        <module>tkg2d</module>
        <module>tkg3d</module>
        <module>tkgeombase</module>
    </parentModules>
    <parts>1</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <functions>
        <!-- <name>^IBPMatrix.*</name> -->
    </functions>
    <exclude>
        <signature>.*::Iterator.*</signature>
        <signature>.*THE_ASCII_VERSIONS.*</signature>
        <!-- error: use of deleted function -->
        <signature>.*TopoDS_AlertAttribute\(const TopoDS_AlertAttribute.*&amp;.*\)</signature>
        <signature>.*Message_AttributeStream\(const Message_AttributeStream.*&amp;.*\)</signature>
        
        <!-- undefined symbol, linux -->
        <signature>.*BinTools_Curve2dSet::Dump.*</signature>
        <!-- OS X internal stuff -->
        <signature>.*qt_mac_set_cursor.*</signature>
    </exclude>
    <classList>
