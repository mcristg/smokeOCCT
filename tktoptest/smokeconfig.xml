<config>
    <moduleName>tktoptest</moduleName>
    <parentModules>
        <module>tkfeat</module>
        <module>tkoffset</module>
        <module>tkv3d</module>
        <module>tkdraw</module>
    </parentModules>
    <parts>1</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <functions>
        <!-- <name>^IBPMatrix.*</name> -->
    </functions>
    <exclude>
        <!-- undefined symbol, linux -->
        <signature>.*BRepTest::PlacementCommands.*</signature>
        <!-- OS X internal stuff -->
        <signature>.*qt_mac_set_cursor.*</signature>
    </exclude>
    <classList>
        <class>handle_BOPTest_DrawableShape</class>
        <class>handle_BRepTest_DrawableHistory</class>
        <class>handle_DrawFairCurve_Batten</class>
        <class>handle_DrawFairCurve_MinimalVariation</class>
        <class>handle_GeometryTest_DrawableQualifiedCurve2d</class>
        <class>handle_HLRTest_DrawableEdgeTool</class>
