<config>
    <moduleName>tkstl</moduleName>
    <parentModules>
        <module>tkernel</module>
        <module>tkmath</module>
        <module>tkxde</module>
        <module>tkg2d</module>
        <module>tkg3d</module>
        <module>tkbrep</module>
        <module>tktopalgo</module>
        <module>tklcaf</module>
        <module>tkxcaf</module>
    </parentModules>
    <parts>1</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <functions>
        <!-- <name>^IBPMatrix.*</name> -->
    </functions>
    <exclude>
        <!-- <signature>.*::Iterator.*</signature> -->
        <!-- OS X internal stuff -->
        <signature>.*qt_mac_set_cursor.*</signature>
    </exclude>
    <classList>
        <class>RWStl</class>
        <class>RWStl_ConfigurationNode</class>
        <class>RWStl_Provider</class>
        <class>RWStl_Reader</class>
        <class>StlAPI</class>
        <class>StlAPI_Reader</class>
        <class>StlAPI_Writer</class>
    </classList>
    <!-- className|meth.toString()|typeName|idx_param -->
    <tripleConditions>
        <tripleCondition>RWStl_Reader|ReadAscii|const std::streampos*|3</tripleCondition>
    </tripleConditions>
</config>
