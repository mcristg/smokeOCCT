<config>
    <moduleName>tkmesh</moduleName>
    <parentModules>
        <module>tkshhealing</module>
    </parentModules>
    <parts>1</parts>
    <excludeIncFiles>
        <excludeIncFile>TCollection_AsciiString.lxx</excludeIncFile>
    </excludeIncFiles>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>opencascade::handle&lt;Geom_Plane&gt;</typeName>
        <typeName>opencascade::handle&lt;Geom_Surface&gt;</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>opencascade::handle&lt;Geom_Plane&gt;</typeName>
        <typeName>opencascade::handle&lt;Geom_Surface&gt;</typeName>
    </voidpTypes>
    <functions>
        <name>^HashCode.*</name>
        <name>^IsEqual.*</name>
    </functions>
    <exclude>
        <!-- undefined symbol, linux -->
        <signature>.*BRepMesh_Triangulator::operator=.*</signature>
        
        <!-- error: invalid new-expression of abstract class type -->
        <signature>.*IMeshData_Edge\(const TopoDS_Edge.*&amp;.*\)</signature>
        <signature>.*IMeshData_Edge\(const IMeshData_Edge.*&amp;.*\)</signature>
        <signature>.*IMeshData_Wire\(const TopoDS_Wire.*&amp;.*\)</signature>
        <signature>.*IMeshData_Wire\(const IMeshData_Wire.*&amp;.*\)</signature>
        
        <signature>.*myFactoryMethods.*</signature>
        
        <!-- OS X internal stuff -->
        <signature>.*qt_mac_set_cursor.*</signature>
    </exclude>
