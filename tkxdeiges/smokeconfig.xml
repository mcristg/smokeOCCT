<config>
    <moduleName>tkxdeiges</moduleName>
    <parentModules>
        <module>tkiges</module>
        <module>tkxde</module>
        <module>tkxcaf</module>
    </parentModules>
    <parts>1</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <functions>
        <!-- <name>^IBPMatrix.*</name> -->
    </functions>
    <exclude>
        <!-- <signature>.*::Iterator.*</signature> -->
        <!-- OS X internal stuff -->
        <signature>.*qt_mac_set_cursor.*</signature>
    </exclude>
    <classList>
        <class>IGESCAFControl</class>
        <class>IGESCAFControl_ConfigurationNode</class>
        <class>IGESCAFControl_Provider</class>
        <class>IGESCAFControl_Reader</class>
        <class>IGESCAFControl_Writer</class>
    </classList>
</config>
