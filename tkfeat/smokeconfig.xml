    <parentModules>
        <module>tkbool</module>
    </parentModules>
    <parts>1</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
    </voidpTypes>
    <functions>
        <!-- <name>^IBPMatrix.*</name> -->
