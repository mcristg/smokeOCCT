<config>
    <moduleName>tkstd</moduleName>
    <parentModules>
        <module>tkcaf</module>
        <module>tkstdl</module>
    </parentModules>
    <parts>1</parts>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <functions>
        <!-- <name>^IBPMatrix.*</name> -->
    </functions>
    <exclude>
        <!-- OS X internal stuff -->
        <signature>.*qt_mac_set_cursor.*</signature>
        
        <signature>.*StdPersistent_TopoDS::pTShape.*</signature>
        <signature>.*myPCurve.*</signature>
        <signature>.*mySurface.*</signature>
        <signature>.*myPolygon2D.*</signature>
        <signature>.*myPolygon.*</signature>
        <signature>.*myTriangulation.*</signature>
    </exclude>
    <classList>
        <class>ShapePersistent</class>
        <class>ShapePersistent_BRep</class>
